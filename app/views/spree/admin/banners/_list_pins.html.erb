<div id="image_pins">
<h1>Listing Image Pins</h1>

<table class="table table-striped table-bordered table-condensed" >
  <thead>
    <tr>
      <th>X1</th>
      <th>Y1</th>
      <th>X2</th>
      <th>Y2</th>
      <th>Image Link</th>
      <th>Image Alt</th>
      <th>Shape</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @pins && @pins.each do |pin| %>
      <tr>
        <td><%= pin.x1 %></td>
        <td><%= pin.y1 %></td>
        <td><%= pin.x2 %></td>
        <td><%= pin.y2%></td>
        <td><%= pin.link %></td>
        <td><%= pin.alt %></td>
        <td><%= pin.shape %></td>
        <td><%= link_to 'Show', "#" %></td>
        <td><%= link_to 'Edit', "#" %></td>
        <td><%= link_to 'Destroy', "#", :confirm => 'Are you sure?', :method => :delete %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br />
</div>

